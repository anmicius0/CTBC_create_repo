# Nexus Repository Manager Configuration
NEXUS_URL=http://your-nexus-server:8081
NEXUS_USERNAME=admin
NEXUS_PASSWORD=your-admin-password
NEXUS_API_PATH=/service/rest

# IQ Server Configuration
IQSERVER_URL=http://your-iqserver:8070
IQSERVER_USERNAME=your-iq-username
IQSERVER_PASSWORD=your-iq-password

# Flask Configuration
PORT=5000
FLASK_SECRET_KEY=your-secret-key-here
FLASK_DEBUG=false
FLASK_HOST=0.0.0.0

# Application Defaults
DEFAULT_PACKAGE_MANAGER=apt
SHARED_PACKAGE_MANAGERS=npm,maven2,nuget,yum,raw

# Default Roles Configuration (comma-separated)
DEFAULT_ROLES=role1,role2  # optional
